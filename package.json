{
  "name": "ai-model-trainer",
  "version": "1.0.0",
  "description": "Web tool for training reasoning prompt enhancer with GPT OSS 20B",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm start",
    "install:all": "pip install -r requirements.txt && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
